import java.util.*;
public class SHUOJ1878 {
	public static void main(String[] args){
		Scanner input=new Scanner(System.in);
		int n=input.nextInt(),m=input.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++)
			a[i]=input.nextInt();
		for(int i=0;i<n;i++){
			int b=input.nextInt();
			int c=Arrays.binarySearch(a, b);
			System.out.println(c);
		}
	}
}
