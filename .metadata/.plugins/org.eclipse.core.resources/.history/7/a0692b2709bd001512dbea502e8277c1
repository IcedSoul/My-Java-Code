import java.util.Arrays;
import java.util.Scanner;
import java.math.*;
public class Main {
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        while(input.hasNext()){
        	int n=input.nextInt(),m=input.nextInt(),k=input.nextInt();
        	double s[]=new double[1000005];
        	double b[]=new double[1000005];
        	for(int i=0;i<n;i++){
        		for(int j=0;j<m;j++)
        			s[j]+=input.nextDouble();
        	}
        	for(int i=0;i<m;i++)
        		b[i]=s[i];
        	Arrays.sort(s,0,m);
        	int ans[]=new int[k];
        	int p=0;
        	for(int i=m-1;i>=m-k;i--){
        		for(int j=0;j<m;j++)
        			if(s[i]==b[j])
        				{ans[p++]=j+1;}
        	}
        	Arrays.sort(ans);
        	for(int i=0;i<k-1;i++){
        		System.out.print(ans[i]+" ");
        	}
        	System.out.println(ans[k-1]);
        }
     }   
}