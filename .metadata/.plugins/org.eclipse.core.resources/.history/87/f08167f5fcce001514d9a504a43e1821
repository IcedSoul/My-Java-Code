import java.util.*;
public class HDU2048 {
	public static void main(String[] args){
		Scanner input=new Scanner(System.in);
		double v=1;
		double ans[]=new double[50];
		ans[1]=0.0;
		double sum=0.0;
		for(int i=2;i<=40;i++){
			sum+=(1.0/(double)(v))*(double)ans[i-1];
			System.out.println();
			v=v*(double)i;
			ans[i]=(long)(v*(1-sum));
			System.out.println(sum+" "+v);
		}
		while(input.hasNext()){
			int n=input.nextInt();
			System.out.println(ans[n]);
		}
	}
}
