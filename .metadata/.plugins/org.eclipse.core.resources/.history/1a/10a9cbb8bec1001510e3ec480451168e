import java.util.Scanner;
public class SHUOJ1223 {
    public static void main(String[] args){
    Scanner input=new Scanner(System.in);
    	while(input.hasNext()){
    		String a=input.next();
    		for(int i=0;i<a.length();i++){
    			char s=a.charAt(i);
    			if(s>'A'&&s<'Z'){
    				if(s=='Z')
    					System.out.print(a);
    				else
    					System.out.println((char)((int)s-32+1));
    			}
    			else if(s>'a'&&s<'z'){
    				if(s>='a'&&s<='c')
    					System.out.print("2");
    				else if(s>='d'&&s<='f')
    					System.out.print("3");
    				else if(s>='g'&&s<='i')
    					System.out.print("4");
    				else if(s>='j'&&s<='l')
    					System.out.print("5");
    				else if(s>='m'&&s<='o')
    					System.out.print("6");
    				else if(s>='p'&&s<='s')
    					System.out.print("7");
    				else if(s>='t'&&s<='v')
    					System.out.print("8");
    				else if(s>='w'&&s<='z')
    					System.out.print("9");
    			}
    		}
    		System.out.println();
    	}
    }
}
