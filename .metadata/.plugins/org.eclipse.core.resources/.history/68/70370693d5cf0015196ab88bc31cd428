import java.util.*;
public class SHUOJ1231 {
	public static void main(String[] args){
		Scanner input=new Scanner(System.in);
		int ans=0;
		while(true){
			double a=input.nextDouble();
			if(a==0.0) break;
			double b=Math.pow(a,1.0/3.0);
			System.out.println(b);
			if(b-(int)b==0) ans++;
		}
		System.out.print(ans);
	}
}
